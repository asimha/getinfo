<%= button_to "Log out", destroy_user_session_path, method: :delete, style: "position: absolute; right: 50px;" %>
<div class="header">
	<div>
	<h1>GetInfo</h1>
		<%= button_to 'Create post', new_post_path, method: "get", class: "myButton" %>
		<%= button_to 'My post', posts_user_posts_path, method: "get", class: "myButton" %>
	</div>
</div>

<div class="div-col">
	<% @posts.each do |post|%>
<div class="box" >
		<h2><%= post.title %></h2>
		<br/>
		<p class="p">
		<%= post.text %>
	</p>
	<%= button_to 'Read More', post_path(post), method: "get" %>
</div>
	<% end %>
</div>