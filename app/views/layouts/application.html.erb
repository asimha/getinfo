<!DOCTYPE html>
<html>
<head>
 <title>GetInfo</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if user_signed_in? %>
    <div class = "logout">
      <%= current_user.email %>
  		<%= button_to "Log out", destroy_user_session_path, method: :delete %>
    </div>
  <% end %>
  <div class="header">
		<h1>GetInfo</h1>
	</div>
<%= yield %>

</body>
</html>
