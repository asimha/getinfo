<%= button_to "Log out", destroy_user_session_path, method: :delete, style: "position: absolute; right: 50px;" %>
<div class="header">
	<div>
	<h1>GetInfo</h1>
		<%= button_to 'Create Blog', new_blog_path, method: "get", class: "myButton" %>
	</div>
</div>

<div class="div-col">
	<% @blogs.each do |blog|%>
<div class="box" >
		<h2><%= blog.title %></h2>
		<br/>
		<p class="p">
		<%= blog.text %>
	</p>
	<%= button_to 'Read More', blog_path(blog), method: "get" %>
</div>
	<% end %>
</div>